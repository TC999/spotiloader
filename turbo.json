{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build":{
            "dependsOn": ["^build"],
            "outputs": ["dist"]
        },
        "start":{
            "dependsOn": ["build"],
            "cache": true
        },
        "dev":{
            "cache": false,
            "persistent": true
        },
        "lint":{},
        "test":{}
    }
}