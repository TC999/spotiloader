{
  "name": "spotiloader",
  "version": "3.0.0",
  "description": "spotify, apple music, yt music, etc music downloader",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jabedzaman/spotiloader.git"
  },
  "scripts": {
    "watch": "pnpm --filter './apps/**' run watch",
    "watch:api": "pnpm --filter './apps/api' run watch",
    "watch:web": "pnpm --filter './apps/web' run watch",
    "build": "pnpm --filter './**' run build",
    "build:packages": "pnpm --filter './packages/**' run build",
    "start": "pnpm --filter './apps/**' run start"
  },
  "author": "jabedzaman <hi@jabed.dev> (https://jabed.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jabedzaman/spotiloader/issues"
  },
  "homepage": "https://github.com/jabedzaman/spotiloader#readme",
  "packageManager": "pnpm@9.14.2"
}